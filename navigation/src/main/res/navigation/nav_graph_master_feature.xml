<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_master_feature"
    app:startDestination="@id/fMaster">

    <fragment
        android:id="@+id/fMaster"
        android:name="bru.oliveir.master.MasterFragment"
        android:label="Master">

        <action
            android:id="@+id/action_master_to_detail"
            app:destination="@id/nav_graph_detail_feature"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim">
            <argument
                android:name="repoName"
                app:argType="string" />
            <argument
                android:name="ownerLogin"
                app:argType="string" />
        </action>

    </fragment>

    <include app:graph="@navigation/nav_graph_detail_feature" />

</navigation>